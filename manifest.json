
{
  "name": "Smart Tabs",
  "description": "Suggests tabs to be opened.",
  "version": "2.0",
  "permissions": [
    "history",
    "activeTab"
  ],
  "background": {
    "scripts": ["suggest.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Open suggested tabs"
  },
  "manifest_version": 2
}